  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "GENERATE_SOURCEMAP=false node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom-sixteen",
    "test:ci": "CI=true npm test --env=jsdom-sixteen",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e:api:start": "./scripts/start-e2e-server",
    "test:e2e:api:clean": "./scripts/clean-e2e-server",
    "test:e2e:run": "start-server-and-test test:e2e:start http://localhost:3001 cy:run",
    "test:e2e:dev": "start-server-and-test test:e2e:start http://localhost:3001 cy:open",
    "test:e2e:start": "PORT=3001 REACT_APP_API_URL=http://localhost:8081/api npm start",
    "pretest": "prettier --list-different \"**/*.js\"",
    "prettier": "prettier --write \"**/*.js\"",
    "eslint": "eslint '**/*.js'",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!**/semantic/**",
      "!src/components/routes/*.js",
      "!src/components/rangeUsePlanPage/pdf/**",
      "!src/store/*.js",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@testing-library/cypress": "^6.0.1",
    "buffer": "^6.0.3",
    "cypress-log-to-output": "^1.1.2",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-testing-library": "^4.0.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-react": "^7.33.2",
    "findup-sync": "^4.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "start-server-and-test": "^1.14.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
